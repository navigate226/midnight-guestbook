{
  "name": "@guestbook/guestbook-contract",
  "type": "module",
  "main": "index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "packageManager": "yarn@4.9.2",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "compact": "compact compile src/guestbook.compact ./src/managed/guestbook",
    "build": "rm -rf dist && tsc --project tsconfig.build.json && cp -R ./src/managed ./dist/managed && cp ./src/guestbook.compact dist",
    "lint": "eslint src",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "prepack": "yarn build"
  },
  "devDependencies": {
    "eslint": "^9.27.0",
    "jest": "^29.7.0",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@midnight-ntwrk/compact-runtime": "^0.9.0"
  }
}
